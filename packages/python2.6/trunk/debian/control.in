Package: @PVER@-dev
Architecture: any
Depends: @PVER@ (= ${binary:Version}), lib@PVER@ (= ${binary:Version}), ${shlibs:Depends}
Recommends: libc6-dev | libc-dev
Replaces: @PVER@ (<< 2.6.2~rc1-0ubuntu2)
